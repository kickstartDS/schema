{
  "$schema": "https://developer.microsoft.com/json-schemas/rush/v5/rush.schema.json",
  "rushVersion": "5.97.1",
  "pnpmVersion": "7.32.2",
  "nodeSupportedVersionRange": ">=18.15.0 <19.0.0",
  "projectFolderMinDepth": 2,
  "projectFolderMaxDepth": 2,
  "ensureConsistentVersions": true,
  "gitPolicy": {
    "versionBumpCommitMessage": "Bump versions [skip ci]",
    "changeLogUpdateCommitMessage": "Update changelogs [skip ci]",
    "allowedEmailRegExps": ["[^@]+@kickstartds\\.com"],
    "sampleEmail": "jonas.ulrich@kickstartds.com"
  },
  "repository": {
    "url": "https://github.com/kickstartDS/schema",
    "defaultBranch": "main",
    "defaultRemote": "origin"
  },
  "eventHooks": {
    "preRushInstall": [],
    "postRushInstall": [],
    "preRushBuild": [],
    "postRushBuild": []
  },
  "variants": [],
  "telemetryEnabled": false,
  "allowedProjectTags": ["tools", "examples"],
  "projects": [
    {
