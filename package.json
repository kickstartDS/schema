{
  "name": "kickstartds-graphql-toolkit",
  "private": true,
  "version": "0.0.1",
  "main": "index.js",
  "license": "UNLICENSED",
  "scripts": {
    "start": "concurrently --kill-others-on-fail \"yarn build-jsonschema2graphql\" \"yarn build-kickstartds-graphql\" \"yarn watch-kickstartds-graphql\"",
    "build-jsonschema2graphql": "yarn workspace jsonschema2graphql watch",
    "build-kickstartds-graphql": "yarn workspace generate-graphql-schema generate",
    "watch-kickstartds-graphql": "yarn workspace generate-graphql-schema generate --watch"
  },
  "workspaces": [
    "generate-graphql-schema",
    "jsonschema2graphql",
    "example"
  ],
  "dependencies": {
    "@kickstartds/base": "^1.0.0-alpha.19",
    "@kickstartds/blog": "^1.0.0-alpha.19",
    "@kickstartds/content": "^1.0.0-alpha.19",
    "@kickstartds/core": "^1.0.0-alpha.14",
    "concurrently": "^6.1.0"
  }
}
