{
  "name": "@kickstartds/jsonschema2graphql",
  "version": "2.4.8",
  "description": "Converts JSON schema to GraphQL types",
  "author": {
    "name": "Jonas Ulrich",
    "email": "jonas.ulrich@ruhmesmeile.com"
  },
  "keywords": [
    "json-schema",
    "graphql"
  ],
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "build"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "clean": "del build",
    "reset": "git clean -dfx && git reset --hard && yarn install",
    "test": "jest",
    "watch": "run-s clean build && run-p \"build -- -w\" \"test --watch\""
  },
  "engines": {
    "node": ">=8.9"
  },
  "dependencies": {
    "@kickstartds/jsonschema-utils": "^2.4.6",
    "ajv": "^8.5.0",
    "camelcase": "^6.3.0",
    "graphql": "^15.8.0",
    "lodash": "^4.17.21",
    "pluralize": "^8.0.0",
    "ramda": "^0.28.0",
    "uppercamelcase": "^3.0.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.13",
    "@types/gh-pages": "^3.2.1",
    "@types/json-schema": "^7.0.11",
    "@types/lodash": "^4.14.182",
    "@types/node": "^16.11.38",
    "@types/pluralize": "^0.0.29",
    "@types/ramda": "^0.28.13",
    "@types/uppercamelcase": "^3.0.0",
    "babel-jest": "^28.1.0",
    "del-cli": "^4.0.1",
    "fs-extra": "^10.1.0",
    "gh-pages": "^4.0.0",
    "jest": "28.1.0",
    "jest-diff": "^28.1.0",
    "jest-matcher-utils": "^28.1.0",
    "npm-run-all": "^4.1.5",
    "opn-cli": "^5.0.0",
    "prettier": "^2.6.2",
    "tmp-promise": "^3.0.3",
    "ts-jest": "28.0.4",
    "typescript": "^4.7.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "roots": [
      "<rootDir>/src",
      "<rootDir>/test"
    ],
    "testEnvironment": "node",
    "testMatch": [
      "**/?(*.)test.ts"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/test/matchers/index.ts",
    "verbose": true
  },
  "prettier": {
    "printWidth": 120,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "restricted"
  }
}
